interface Profile {
  name: string;
  age: number;
  email: string;
}

const updateProfile = (
  original: Profile,
  changes: Partial<Profile>,
): Profile => {
  const updated = {
    ...original,
    ...changes,
  };
  return updated;
};

// Sample Input
const myProfile = { name: "Alice", age: 20, email: "alice@example.com" };
console.log(updateProfile(myProfile, { age: 36 }));
